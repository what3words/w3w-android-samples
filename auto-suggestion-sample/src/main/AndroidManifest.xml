            <!--                <action android:name="android.intent.action.MAIN" />-->

            <!--                <category android:name="android.intent.category.LAUNCHER" />-->
            <!--            </intent-filter>-->
        </activity>
        <activity
            android:name="com.what3words.samples.autosuggestion.xml.MainActivity"
            android:exported="true"
            android:theme="@style/AppTheme">
            <!-- Uncomment here to run XML layout activity sample. Do not forget to comment the block from line 20 to 24 -->
